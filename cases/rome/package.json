    "https": false,
    "readline": false,
    "child_process": false,
    "tty": false,
    "process": false,
    "buffer": false
  },
  "devDependencies": {
    "@parcel/config-default": "^2.8.3",
    "@parcel/optimizer-swc": "^2.8.3",
    "@parcel/resolver-default": "^2.8.3",
    "@rspack/cli": "0.0.0-82fa99600-20230219083939",
    "@swc/core": "^1.3.35",
    "esbuild-loader": "^3.0.0",
