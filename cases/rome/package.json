  "source": "./src/index.ts",
  "scripts": {
    "bench:setup": "node -e \"try { require('fs').statSync('src/rome') } catch { process.exitCode = 1 }\" || git clone --single-branch --branch v10.0.4-beta https://github.com/romejs/rome src/rome",
