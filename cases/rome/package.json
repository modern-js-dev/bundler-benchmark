{
  "name": "rome",
  "dependenciesDate": "2021-02-15",
  "dependencies": {
    "webpack-cli": "^4.5.0"
  },
  "alias": {
    "@internal/*": "./src/rome/internal/$1",
    "inspector": false,
    "crypto": false,
    "fs": false,
    "os": false,
    "module": false,
    "vm": false,
    "net": false,
    "zlib": false,
